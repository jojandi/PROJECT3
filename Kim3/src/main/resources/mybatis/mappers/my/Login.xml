<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="project3.gamja.Kim.dao.LoginDAO">
	<select id="selectLogin" resultType="LoginDTO"  parameterType="int" >
	
	SELECT 
		tbl_user 
	WHERE
		user_id = #{user_id},
		user_pw = #{user_pw}
</select>

	
	
	
</mapper>